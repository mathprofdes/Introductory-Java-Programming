import java.awt.*;
import javax.swing.*;

public class IntroGUI020 extends JFrame {

	private static IntroGUI020 prog;
	private GraphicsJPanel canvas;

	public static void main(String[] args) {
		prog = new IntroGUI020(args);
		prog.setTitle("GUI");

		prog.setBounds(20, 20, 507, 429);
		prog.setResizable(false);
		prog.setVisible(true);
		prog.toFront();
	}

	public IntroGUI020(String[] args) {
		canvas = new GraphicsJPanel();

		getContentPane().setLayout(new BorderLayout());
		getContentPane().add(canvas, BorderLayout.CENTER);
	}
}
